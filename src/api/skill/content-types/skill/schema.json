{
  "kind": "collectionType",
  "collectionName": "skills",
  "info": {
    "singularName": "skill",
    "pluralName": "skills",
    "displayName": "skill",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "link": {
      "displayName": "link",
      "type": "component",
      "repeatable": false,
      "component": "element.link"
    },
    "featured": {
      "type": "enumeration",
      "enum": [
        "on",
        "off"
      ]
    },
    "skill_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::skill-category.skill-category",
      "inversedBy": "skills"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    }
  }
}
