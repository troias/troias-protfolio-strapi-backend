{
  "kind": "collectionType",
  "collectionName": "skills",
  "info": {
    "singularName": "skill",
    "pluralName": "skills",
    "displayName": "skill"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "body": {
      "type": "richtext"
    },
    "cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "link": {
      "displayName": "link",
      "type": "component",
      "repeatable": false,
      "component": "element.link"
    },
    "skill_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill-category.skill-category",
      "inversedBy": "skills"
    },
    "project_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project-category.project-category",
      "inversedBy": "skills"
    }
  }
}
